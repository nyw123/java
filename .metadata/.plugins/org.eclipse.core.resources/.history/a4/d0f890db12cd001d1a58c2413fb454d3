package ch07.sec05.exam02;

public class FP {
	public static void main(String[] args) {
		Driver driver = new Driver();
		Bus bus = new Bus();
		
		driver.driver(bus);
	}
}

class Driver{
	void driver(Vehicle v) {
		
	}
}
class Vehicle{void run(){System.out.println("운송수단운전");}}
class Bus extends Vehicle{

	@Override
	void run() {
		System.out.println("Bus를 운전");
	}}